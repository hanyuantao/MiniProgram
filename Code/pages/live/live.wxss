/* message.wxss */

.liveContainer {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  box-sizing: border-box;
}


.rankTitle {
  margin: 50rpx;
  align-items: center;
  color: #600;
  font-size: 30;
  text-align: center;
}

.teamCell {
  margin: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.team {
  margin: 10rpx;
  color: #110;
  font-size: 16;
  text-align: left;
}

.rank {
  margin: 10rpx;
  color: #F00;
  size: 20;
  text-align: right;
}
